#

sudo cp /etc/jailkit/jk_init.ini.original /etc/jailkit/jk_init.ini
echo "" | sudo tee -a /etc/jailkit/jk_init.ini >> /dev/null
cat /etc/scripts/etc.jailkit.jk_init.ini.template | sudo tee -a /etc/jailkit/jk_init.ini >> /dev/null

